{
  "name": "example",
  "productName": "React Native Electron",
  "version": "1.0.0",
  "description": "Example React Native Electron application",
  "author": {
    "name": "Paul Le Cam",
    "email": "paul@ulem.net"
  },
  "keywords": [],
  "license": "MIT",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "make": "electron-forge make",
    "package": "electron-forge package"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native-electron": "workspace:*"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-webpack": "7.5.0",
    "electron": "^33.2.0",
    "node-loader": "^2.1.0",
    "swc-loader": "^0.2.6",
    "webpack": "^5.96.1"
  },
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ],
      "packagerConfig": {},
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "preload": {
                    "js": "./node_modules/react-native-electron/lib/preload.js"
                  },
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ]
    }
  }
}
